  "dependencies": {
    "@directus/gatsby-source-directus": "~0.2.6",
    "@theme-ui/color": "~0.3.1",
    "@theme-ui/presets": "~0.3.0",
    "gatsby": "~2.23.11",
    "gatsby-image": "~2.4.9",
    "gatsby-plugin-manifest": "~2.4.14",
    "gatsby-plugin-netlify": "~2.3.7",
    "gatsby-plugin-offline": "~3.2.13",
    "gatsby-plugin-postcss": "~2.3.6",
    "gatsby-plugin-react-helmet": "~3.3.6",
    "gatsby-plugin-sharp": "~2.6.14",
    "gatsby-plugin-sitemap": "~2.4.7",
    "gatsby-source-filesystem": "~2.3.14",
    "gatsby-transformer-sharp": "~2.5.7",
    "i18next": "~19.5.1",
    "i18next-fs-backend": "~1.0.6",
    "prop-types": "~15.7.2",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-helmet": "~6.1.0",
    "react-i18next": "~11.7.0",
    "react-spring": "~8.0.27",
    "theme-ui": "~0.3.1"
  },
  "devDependencies": {
    "cross-env": "~7.0.2",
    "dotenv": "~8.2.0",
    "eslint": "~7.3.1",
    "eslint-config-fmal": "~7.1.0",
    "eslint-plugin-jsx-a11y": "~6.3.1",
    "eslint-plugin-react": "~7.20.0",
    "eslint-plugin-react-hooks": "~4.0.4",
    "husky": "~4.2.5",
    "lint-staged": "~10.2.11",
    "npm-check": "~5.9.2",
    "prettier": "2.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:incremental": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "gatsby develop",
    "develop:playground": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "lint": "eslint . --cache --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deps": "npm-check -u",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fmal/wyspa-www"
  }
