{
  "name": "wyspa-www",
  "private": true,
  "description": "Wyspa WWW",
  "version": "0.1.0",
  "author": "Filip Malinowski <filip@fmal.me>",
  "dependencies": {
    "@directus/gatsby-source-directus": "~0.2.6",
    "gatsby": "^2.22.15",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-netlify": "~2.3.4",
    "gatsby-plugin-offline": "^3.2.7",
    "gatsby-plugin-postcss": "~2.3.3",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-plugin-sitemap": "~2.4.5",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-sharp": "^2.5.3",
    "i18next": "~19.4.5",
    "i18next-fs-backend": "~1.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-i18next": "~11.5.1"
  },
  "devDependencies": {
    "cross-env": "~7.0.2",
    "cssnano": "~4.1.10",
    "dotenv": "~8.2.0",
    "eslint": "~6.8.0",
    "eslint-config-fmal": "~7.0.0",
    "eslint-plugin-jsx-a11y": "~6.2.3",
    "eslint-plugin-react": "~7.20.0",
    "husky": "~4.2.5",
    "lint-staged": "~10.2.9",
    "postcss-preset-env": "~6.7.0",
    "prettier": "2.0.5",
    "tailwindcss": "~1.4.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:incremental": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "gatsby develop",
    "develop:playground": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "lint": "eslint . --cache --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fmal/wyspa-www"
  }
}
